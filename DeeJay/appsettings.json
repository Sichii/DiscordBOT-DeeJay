{
  "Options": {
    "DiscordClientServiceOptions": {
      "Token": ""
    },
    "GuildOptionsRepositoryOptions": {
      "Directory": "GuildOptions"
    },
    "YtdlSearchServiceOptions": {
      "ProxyUrl": null
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Debug",
      "Microsoft": "Debug"
    },
    "NLog": {
      "throwConfigExceptions": true,
      "targets": {
        "async": true,
        "logconsole": {
          "type": "ColoredConsole",
          "layout": "${pad:padding=-21:fixedLength=true:${time}|${uppercase:${level}}} ${logger:shortName=true:padding=25:fixedLength=true}: ${message} ${exception:format=tostring}",
          "rowHighlightingRules": [
            {
              "condition": "level == LogLevel.Fatal",
              "foregroundColor": "Red",
              "backgroundColor": "White"
            },
            {
              "condition": "level == LogLevel.Error",
              "foregroundColor": "Red"
            },
            {
              "condition": "level == LogLevel.Info",
              "foregroundColor": "White"
            },
            {
              "condition": "level == LogLevel.Debug",
              "foregroundColor": "Blue"
            }
          ]
        },
        "logfile": {
          "type": "File",
          "fileName": "${basedir}\\logs\\${shortdate}.log",
          "layout": "${pad:padding=-21:fixedLength=true:${time}|${uppercase:${level}}} ${logger:shortName=true:padding=25:fixedLength=true}: ${message} ${exception:format=tostring}",
          "archiveFileName": "${basedir}\\logs\\archive\\{#}.log",
          "archiveNumbering": "DateAndSequence",
          "archiveDateFormat": "yyyy-MM-dd",
          "maxArchiveFiles": 10,
          "archiveEvery": "Day",
          "archiveOldFileOnStartup": true,
          "keepFileOpen": true
        }
      },
      "rules": [
        {
          "logger": "*",
          "minLevel": "Debug",
          "writeTo": "logconsole"
        },
        {
          "logger": "*",
          "minLevel": "Trace",
          "writeTo": "logfile"
        }
      ]
    }
  }
}